{
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-node-env-inline",
    "transform-flow-strip-types"
  ],
  "presets": [
    [
      "env",
      {
        "loose": false,
        "modules": false,
        "targets": {
          "chrome": 58
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ]
}
